CREATE SEQUENCE MESSAGE_SEQ START WITH 50000;

CREATE OR REPLACE LANGUAGE plpgsql;
CREATE OR REPLACE FUNCTION addMsg()
	RETURNS "trigger" AS
	$BODY$
	BEGIN
    NEW.msgId = nextval('MESSAGE_SEQ');
	RETURN NEW;
	END
	$BODY$
	LANGUAGE plpgsql VOLATILE;

CREATE TRIGGER part_num_trigger BEFORE INSERT ON MESSAGE FOR EACH ROW EXECUTE PROCEDURE addMsg();